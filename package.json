{
  "name": "slonoed.net",
  "private": true,
  "description": "",
  "version": "1.0.0",
  "author": "Dmitry Manannikov <email@slonoed.net>",
  "dependencies": {
    "gatsby": "2.1.19",
    "gatsby-plugin-catch-links": "2.0.12",
    "gatsby-plugin-feed": "2.0.14",
    "gatsby-plugin-google-analytics": "2.0.15",
    "gatsby-plugin-i18n": "1.0.1",
    "gatsby-plugin-manifest": "2.0.20",
    "gatsby-plugin-offline": "2.0.24",
    "gatsby-plugin-react-helmet": "3.0.7",
    "gatsby-plugin-sharp": "2.0.23",
    "gatsby-plugin-typography": "2.2.7",
    "gatsby-remark-autolink-headers": "2.0.15",
    "gatsby-remark-copy-linked-files": "2.0.9",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "3.0.5",
    "gatsby-remark-prismjs": "3.2.4",
    "gatsby-remark-responsive-iframe": "2.0.9",
    "gatsby-remark-smartypants": "2.0.8",
    "gatsby-source-filesystem": "2.0.23",
    "gatsby-transformer-remark": "2.3.0",
    "gatsby-transformer-sharp": "2.1.15",
    "husky": "^1.3.1",
    "lint-staged": "8.1.4",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "r-dom": "^2.4.0",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.13",
    "remark-inline-links": "^3.1.2",
    "remark-reference-links": "^4.0.3",
    "typography": "^0.16.17",
    "typography-theme-wordpress-2016": "0.16.18"
  },
  "devDependencies": {
    "eslint": "5.14.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.1",
    "prettier": "1.16.4"
  },
  "homepage": "https://slonoed.net",
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "now-build": "gatsby build",
    "deploy": "yarn build && gh-pages -d public -b master",
    "dry": "yarn build && cp now.json public/ && cd public && now"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  }
}
